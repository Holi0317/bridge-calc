{
  "name": "bridge-calc",
  "version": "0.1.0",
  "description": "Webapp for calculating mark in German Bridge",
  "author": "Holi0317 <holliswuhollis@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/Holi0317/bridge-calc.git"
  },
  "bugs": {
    "url": "https://github.com/Holi0317/bridge-calc/issues"
  },
  "homepage": "https://github.com/Holi0317/bridge-calc#readme",
  "main": "dist/main.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test ava",
    "lint": "npm run lint:js",
    "lint:js": "eslint --ext .jsx --ext .js --cache webpack-cfg test webpack.config.js src",
    "clean": "del-cli dist",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean",
    "build:dev": "cross-env NODE_ENV=development npm run webpack -- --progress --profile",
    "prebuild:prod": "npm run clean",
    "build:prod": "cross-env NODE_ENV=production npm run webpack -- --progress --profile",
    "build-splash-css": "postcss styles/splash.css --no-map -u cssnano postcss-cssnext",
    "start": "npm run server:dev",
    "server": "npm run server:dev",
    "server:dev": "cross-env NODE_ENV=development npm run webpack-dev-server -- --inline --progress --profile --watch",
    "server:prod": "serve -s -p 8080 dist",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server"
  },
  "ava": {
    "files": [
      "test/utils.spec.js"
    ],
    "failFast": true,
    "require": "babel-register",
    "babel": "inherit"
  },
  "dependencies": {
    "bootstrap": "4.0.0-alpha.6",
    "decko": "1.2.0",
    "localforage": "1.5.0",
    "localforage-memoryStorageDriver": "0.9.2",
    "preact": "8.1.0",
    "preact-compat": "3.14.3",
    "preact-delegate": "1.1.1",
    "preact-redux": "2.0.1",
    "react-collapse": "2.3.3",
    "react-height": "2.2.0",
    "react-icons": "2.2.3",
    "react-motion": "0.4.7",
    "react-router": "4.0.0",
    "react-router-dom": "4.0.0",
    "react-toolbox": "2.0.0-beta.8",
    "redux": "3.6.0"
  },
  "devDependencies": {
    "@easy-webpack/core": "2.0.1",
    "ava": "0.19.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.1",
    "babel-loader": "6.4.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.1",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.3.3",
    "babel-preset-babili": "0.0.12",
    "babel-preset-es2016": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "babili-webpack-plugin": "0.0.11",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "4.0.0",
    "css-loader": "0.28.0",
    "cssnano": "3.10.0",
    "del-cli": "0.2.1",
    "eslint": "3.19.0",
    "eslint-plugin-react": "6.10.3",
    "extract-text-webpack-plugin": "2.1.0",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "postcss-cli": "3.1.1",
    "postcss-cssnext": "2.10.0",
    "postcss-each": "0.9.3",
    "postcss-import": "9.1.0",
    "postcss-loader": "1.3.3",
    "postcss-mixins": "5.4.1",
    "serve": "5.1.2",
    "style-loader": "0.16.1",
    "webpack": "2.3.3",
    "webpack-bundle-analyzer": "2.3.1",
    "webpack-dev-server": "2.4.2",
    "webpack-md5-hash": "0.0.5"
  }
}
