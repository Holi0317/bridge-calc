    "@types/lodash-es": "4.17.0",
    "@types/material-ui": "0.20.5",
    "@types/node": "9.4.5",
    "@types/react": "16.0.36",
    "@types/react-dom": "16.0.3",
    "@types/react-i18next": "7.3.0",
    "@types/react-loadable": "5.3.2",
    "@types/react-redux": "5.0.14",
