{
  "name": "bridge-calc",
  "version": "0.1.0",
  "description": "Webapp for calculating mark in German Bridge",
  "author": "Holi0317 <holliswuhollis@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/Holi0317/bridge-calc.git"
  },
  "bugs": {
    "url": "https://github.com/Holi0317/bridge-calc/issues"
  },
  "homepage": "https://github.com/Holi0317/bridge-calc#readme",
  "main": "dist/main.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test ./node_modules/karma/bin/karma start test/karma.conf.js",
    "lint": "npm run lint:js && npm run lint:ts",
    "lint:js": "eslint --cache webpack-cfg test webpack.config.js",
    "lint:ts": "tslint --project tsconfig.json --type-check",
    "clean": "del-cli dist",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean",
    "build:dev": "cross-env NODE_ENV=development npm run webpack -- --progress --profile",
    "prebuild:prod": "npm run clean",
    "build:prod": "cross-env NODE_ENV=production npm run webpack -- --progress --profile",
    "build-splash-css": "node-sass styles/splash.scss | cssnano",
    "start": "npm run server:dev",
    "server": "npm run server:dev",
    "server:dev": "cross-env NODE_ENV=development npm run webpack-dev-server -- --inline --progress --profile --watch",
    "server:prod": "serve -s -p 8080 dist",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server"
  },
  "dependencies": {
    "decko": "1.2.0",
    "localforage": "1.5.0",
    "localforage-memoryStorageDriver": "0.9.2",
    "material-ui": "0.17.1",
    "preact": "7.2.1",
    "preact-compat": "3.14.3",
    "preact-delegate": "1.1.1",
    "preact-redux": "2.0.1",
    "react-router": "4.0.0",
    "react-router-redux": "4.0.8",
    "redux": "3.6.0",
    "tslib": "1.6.0"
  },
  "devDependencies": {
    "@easy-webpack/core": "2.0.1",
    "@types/material-ui": "0.16.60",
    "@types/node": "7.0.12",
    "@types/react": "15.0.21",
    "@types/react-dom": "0.14.23",
    "awesome-typescript-loader": "3.1.2",
    "babel-preset-babili": "0.0.12",
    "babili-webpack-plugin": "0.0.11",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "3.2.4",
    "css-loader": "0.27.3",
    "cssnano-cli": "1.0.5",
    "del-cli": "0.2.1",
    "eslint": "3.18.0",
    "eslint-plugin-tape": "1.1.0",
    "extract-text-webpack-plugin": "2.1.0",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "karma": "1.5.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-tap": "3.1.1",
    "karma-webpack": "2.0.3",
    "node-sass": "4.5.2",
    "sass-loader": "6.0.3",
    "serve": "5.1.2",
    "style-loader": "0.16.1",
    "tape": "4.6.3",
    "tslint": "4.5.1",
    "tslint-eslint-rules": "3.5.1",
    "typescript": "2.2.2",
    "webpack": "2.3.2",
    "webpack-bundle-analyzer": "2.3.1",
    "webpack-dev-server": "2.4.2",
    "webpack-md5-hash": "0.0.5"
  }
}
