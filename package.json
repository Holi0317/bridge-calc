{
  "name": "bridge-calc",
  "version": "0.1.0",
  "description": "Webapp for calculating mark in German Bridge",
  "author": "Holi0317 <holliswuhollis@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/Holi0317/bridge-calc.git"
  },
  "bugs": {
    "url": "https://github.com/Holi0317/bridge-calc/issues"
  },
  "homepage": "https://github.com/Holi0317/bridge-calc#readme",
  "main": "dist/main.js",
  "scripts": {
    "test": "echo 'Test is disabled. Original command: cross-env NODE_ENV=test ./node_modules/karma/bin/karma start test/karma.conf.js'",
    "lint": "npm run lint:js",
    "lint:js": "eslint --ext .jsx --ext .js --cache webpack-cfg test webpack.config.js src",
    "clean": "del-cli dist",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean",
    "build:dev": "cross-env NODE_ENV=development npm run webpack -- --progress --profile",
    "prebuild:prod": "npm run clean",
    "build:prod": "cross-env NODE_ENV=production npm run webpack -- --progress --profile",
    "build-splash-css": "node-sass styles/splash.scss | cssnano",
    "start": "npm run server:dev",
    "server": "npm run server:dev",
    "server:dev": "cross-env NODE_ENV=development npm run webpack-dev-server -- --inline --progress --profile --watch",
    "server:prod": "serve -s -p 8080 dist",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server"
  },
  "dependencies": {
    "bootstrap": "4.0.0-alpha.6",
    "decko": "1.2.0",
    "localforage": "1.5.0",
    "localforage-memoryStorageDriver": "0.9.2",
    "preact": "8.1.0",
    "preact-compat": "3.14.3",
    "preact-delegate": "1.1.1",
    "react-icons": "2.2.3",
    "react-redux": "5.0.4",
    "react-router": "4.0.0",
    "react-router-dom": "4.0.0",
    "react-toolbox": "2.0.0-beta.8",
    "redux": "3.6.0"
  },
  "devDependencies": {
    "@easy-webpack/core": "2.0.1",
    "babel-core": "6.24.1",
    "babel-loader": "6.4.1",
    "babel-preset-babili": "0.0.12",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babili-webpack-plugin": "0.0.11",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "4.0.0",
    "css-loader": "0.28.0",
    "cssnano-cli": "1.0.5",
    "del-cli": "0.2.1",
    "eslint": "3.19.0",
    "eslint-plugin-react": "6.10.3",
    "eslint-plugin-tape": "1.1.0",
    "extract-text-webpack-plugin": "2.1.0",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "karma": "1.6.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-tap": "3.1.1",
    "karma-webpack": "2.0.3",
    "node-sass": "4.5.2",
    "sass-loader": "6.0.3",
    "serve": "5.1.2",
    "style-loader": "0.16.1",
    "tape": "4.6.3",
    "webpack": "2.3.3",
    "webpack-bundle-analyzer": "2.3.1",
    "webpack-dev-server": "2.4.2",
    "webpack-md5-hash": "0.0.5"
  }
