{
  "name": "bridge-calc",
  "version": "2.2.2",
  "description": "Webapp for calculating mark in German Bridge",
  "author": "Holi0317 <holliswuhollis@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://gitlab.com/holi0317/bridge-calc.git"
  },
  "bugs": {
    "url": "https://gitlab.com/holi0317/bridge-calc/issues"
  },
  "homepage": "https://gitlab.com/holi0317/bridge-calc",
  "main": "dist/main.js",
  "scripts": {
    "test": "jest",
    "lint": "npm run lint:js && npm run lint:ts && npm run lint:css",
    "lint:css": "stylelint --cache --ignore-path .gitignore \"**/*.{css,scss,sass,pcss}\"",
    "lint:css:fix": "npm run lint:css -- --fix",
    "lint:js": "eslint --ext .jsx,.js --ignore-path .gitignore --cache .",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:ts": "tslint -c tslint.json -p tsconfig.json -t stylish",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "prettier": "prettier --ignore-path .gitignore -l \"**/*.{js,scss,sass,css,json,md,yml,yaml,pcss,ts,tsx}\"",
    "prettier:fix": "npm run prettier -- --write",
    "clean": "del-cli dist .tmp",
    "build": "npm run build:prod",
    "prebuild:prod": "npm run clean",
    "build:prod": "cross-env NODE_ENV=production npm run webpack -- --profile --mode production",
    "postbuild:prod": "cp dist/index.html dist/404.html && cp dist/sw.js dist/service-worker.js",
    "build-splash-css": "postcss assets/splash.css --no-map -u cssnano postcss-cssnext",
    "start": "npm run server:dev",
    "server": "npm run server:dev",
    "server:dev": "cross-env NODE_ENV=development npm run webpack-dev-server -- --inline --profile --watch",
    "server:prod": "serve -s -p 8080 dist",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server --mode development",
    "type-check": "tsc --noEmit",
    "ci": "npm run lint && npm test && npm run type-check"
  },
  "dependencies": {
    "@material-ui/core": "3.9.3",
    "@material-ui/icons": "3.0.2",
    "array-move": "2.1.0",
    "date-fns": "1.30.1",
    "history": "4.9.0",
    "i18next": "15.1.0",
    "i18next-browser-languagedetector": "3.0.1",
    "i18next-chained-backend": "1.0.1",
    "i18next-localstorage-backend": "2.1.2",
    "i18next-xhr-backend": "2.0.1",
    "lodash-es": "4.17.11",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-i18next": "10.9.0",
    "react-loadable": "5.5.0",
    "react-redux": "7.0.3",
    "react-router": "5.0.0",
    "react-router-dom": "5.0.0",
    "react-sortable-hoc": "1.9.1",
    "redux": "4.0.1",
    "redux-persist": "5.10.0",
    "reselect": "4.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.4.4",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-stage-1": "7.0.0",
    "@types/i18next": "12.1.0",
    "@types/jest": "24.0.12",
    "@types/lodash-es": "4.17.3",
    "@types/node": "12.0.0",
    "@types/react": "16.8.17",
    "@types/react-collapse": "4.0.2",
    "@types/react-dom": "16.8.4",
    "@types/react-loadable": "5.5.1",
    "@types/react-redux": "7.0.8",
    "@types/react-router-dom": "4.3.3",
    "@types/react-sortable-hoc": "0.6.5",
    "@types/react-transition-group": "2.9.1",
    "awesome-typescript-loader": "5.2.1",
    "babel-minify-webpack-plugin": "0.3.1",
    "copy-webpack-plugin": "5.0.3",
    "cross-env": "5.2.0",
    "css-loader": "2.1.1",
    "cssnano": "4.1.10",
    "del-cli": "1.1.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.2.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "husky": "2.2.0",
    "jest": "24.8.0",
    "lolex": "4.0.1",
    "mini-css-extract-plugin": "0.6.0",
    "postcss": "7.0.16",
    "postcss-advanced-variables": "3.0.0",
    "postcss-cli": "6.1.2",
    "postcss-extend": "1.0.5",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-nested": "4.1.2",
    "postcss-preset-env": "6.6.0",
    "postcss-reporter": "6.0.1",
    "postcss-scss": "2.0.0",
    "postcss-strip-inline-comments": "0.1.5",
    "prettier": "1.17.0",
    "pretty-quick": "1.10.0",
    "serve": "11.0.0",
    "style-loader": "0.23.1",
    "stylelint": "10.0.1",
    "stylelint-config-recommended": "2.2.0",
    "ts-jest": "24.0.2",
    "tslint": "5.16.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.4.5",
    "webpack": "4.31.0",
    "webpack-bundle-analyzer": "3.3.2",
    "webpack-cli": "3.3.2",
    "webpack-dev-server": "3.3.1",
    "webpack-md5-hash": "0.0.6",
    "webpack-merge": "4.2.1",
    "workbox-webpack-plugin": "4.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
