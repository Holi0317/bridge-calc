{
  "name": "bridge-calc",
  "version": "0.1.0",
  "description": "Webapp for calculating mark in German Bridge",
  "author": "Holi0317 <holliswuhollis@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/Holi0317/bridge-calc.git"
  },
  "bugs": {
    "url": "https://github.com/Holi0317/bridge-calc/issues"
  },
  "homepage": "https://github.com/Holi0317/bridge-calc#readme",
  "main": "dist/main.js",
  "scripts": {
    "pretest": "npm run build:test",
    "test": "cross-env NODE_ENV=test ava .tmp/test",
    "lint": "npm run lint:js",
    "lint:js": "eslint --ext .jsx,.js --cache webpack-cfg test webpack.config.js src",
    "lint:js:fix": "eslint --ext .jsx,.js --fix --cache webpack-cfg test webpack.config.js src",
    "flow": "flow check",
    "clean": "del-cli dist .tmp",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean",
    "build:dev": "cross-env NODE_ENV=development npm run webpack -- --progress --profile",
    "prebuild:prod": "npm run clean",
    "build:prod": "cross-env NODE_ENV=production npm run webpack -- --progress --profile",
    "prebuild:test": "npm run clean",
    "build:test": "npm run build:test:src && npm run build:test:test",
    "build:test:src": "cross-env NODE_ENV=test babel src --out-dir .tmp/src",
    "build:test:test": "cross-env NODE_ENV=test babel test --out-dir .tmp/test",
    "build-splash-css": "postcss splash.css --no-map -u cssnano postcss-cssnext",
    "start": "npm run server:dev",
    "server": "npm run server:dev",
    "server:dev": "cross-env NODE_ENV=development npm run webpack-dev-server -- --inline --progress --profile --watch",
    "server:prod": "serve -s -p 8080 dist",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "ci": "npm run lint && npm run flow && npm test"
  },
  "ava": {
    "failFast": true
  },
  "dependencies": {
    "@material/snackbar": "0.3.5",
    "@material/textfield": "0.3.6",
    "i18next": "9.0.0",
    "i18next-browser-languagedetector": "2.0.0",
    "i18next-localstorage-cache": "1.1.1",
    "i18next-xhr-backend": "1.4.2",
    "lodash-es": "4.17.4",
    "preact": "8.2.4",
    "preact-compat": "3.17.0",
    "preact-material-components": "1.1.4",
    "preact-redux": "2.0.2",
    "react-collapse": "4.0.3",
    "react-height": "3.0.0",
    "react-i18next": "5.1.0",
    "react-icons": "2.2.5",
    "react-motion": "0.5.0",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-sortable-hoc": "0.6.7",
    "redux": "3.7.2",
    "reselect": "3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "7.1.2",
    "ava": "0.22.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-module-resolver": "2.7.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-plugin-transform-node-env-inline": "0.2.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.8",
    "babel-preset-es2016": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "5.0.5",
    "css-loader": "0.28.5",
    "cssnano": "3.10.0",
    "del-cli": "1.1.0",
    "eslint": "4.5.0",
    "eslint-plugin-ava": "4.2.1",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-react": "7.3.0",
    "extract-text-webpack-plugin": "3.0.0",
    "flow-bin": "0.53.1",
    "html-loader": "0.5.1",
    "html-webpack-plugin": "2.30.1",
    "postcss-cli": "4.1.0",
    "postcss-import": "10.0.0",
    "postcss-loader": "2.0.6",
    "postcss-nesting": "4.1.0",
    "postcss-reporter": "5.0.0",
    "redux-logger": "3.0.6",
    "serve": "6.0.6",
    "style-loader": "0.18.2",
    "webpack": "3.5.5",
    "webpack-bundle-analyzer": "2.9.0",
    "webpack-dev-server": "2.7.1",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "4.1.0",
    "yaml-loader": "0.5.0"
  }
}
