{
  "name": "bridge-calc",
  "version": "2.1.1",
  "description": "Webapp for calculating mark in German Bridge",
  "author": "Holi0317 <holliswuhollis@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://gitlab.com/holi0317/bridge-calc.git"
  },
  "bugs": {
    "url": "https://gitlab.com/holi0317/bridge-calc/issues"
  },
  "homepage": "https://gitlab.com/holi0317/bridge-calc",
  "main": "dist/main.js",
  "scripts": {
    "test": "jest",
    "lint": "npm run lint:js && npm run lint:ts && npm run lint:css",
    "lint:css": "stylelint src/**/*.css splash.css",
    "lint:js": "eslint --ext .jsx,.js --cache webpack-cfg test *.config.js src",
    "lint:js:fix": "eslint --ext .jsx,.js --fix --cache webpack-cfg test *.config.js src",
    "lint:ts": "tslint -c tslint.json -p tsconfig.json -t stylish",
    "clean": "del-cli dist .tmp",
    "build": "npm run build:prod",
    "prebuild:prod": "npm run clean",
    "build:prod": "cross-env NODE_ENV=production npm run webpack -- --progress --profile",
    "postbuild:prod": "cp dist/index.html dist/404.html && cp dist/sw.js dist/service-worker.js",
    "build-splash-css": "postcss splash.css --no-map -u cssnano postcss-cssnext",
    "start": "npm run server:dev",
    "server": "npm run server:dev",
    "server:dev": "cross-env NODE_ENV=development npm run webpack-dev-server -- --inline --progress --profile --watch",
    "server:prod": "serve -s -p 8080 dist",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "type-check": "tsc --noEmit",
    "ci": "npm run lint && npm test && npm run type-check"
  },
  "dependencies": {
    "cuid": "2.1.0",
    "date-fns": "1.29.0",
    "history": "4.7.2",
    "i18next": "10.3.0",
    "i18next-browser-languagedetector": "2.1.0",
    "i18next-localstorage-cache": "1.1.1",
    "i18next-xhr-backend": "1.5.1",
    "lodash-es": "4.17.5",
    "material-ui": "0.20.0",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-grid-system": "3.1.2",
    "react-height": "3.0.0",
    "react-i18next": "7.3.6",
    "react-loadable": "5.3.1",
    "react-redux": "5.0.6",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-sortable-hoc": "0.6.8",
    "redux": "3.7.2",
    "redux-persist": "5.7.0",
    "reselect": "3.0.1"
  },
