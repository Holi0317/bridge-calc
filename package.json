{
  "name": "bridge-calc",
  "version": "2.2.2",
  "description": "Webapp for calculating mark in German Bridge",
  "author": "Holi0317 <holliswuhollis@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://gitlab.com/holi0317/bridge-calc.git"
  },
  "bugs": {
    "url": "https://gitlab.com/holi0317/bridge-calc/issues"
  },
  "homepage": "https://gitlab.com/holi0317/bridge-calc",
  "main": "dist/main.js",
  "scripts": {
    "test": "jest",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint --cache --ignore-path .gitignore \"**/*.{css,scss,sass,pcss}\"",
    "lint:css:fix": "npm run lint:css -- --fix",
    "lint:js": "eslint --ext .jsx,.js,.ts,.tsx --ignore-path .gitignore --cache .",
    "lint:js:fix": "npm run lint:js -- --fix",
    "prettier": "prettier --ignore-path .gitignore -l \"**/*.{js,scss,sass,css,json,md,yml,yaml,pcss,ts,tsx}\"",
    "prettier:fix": "npm run prettier -- --write",
    "clean": "del-cli dist .tmp",
    "build": "npm run build:prod",
    "prebuild:prod": "npm run clean",
    "build:prod": "cross-env NODE_ENV=production npm run webpack -- --profile --mode production",
    "postbuild:prod": "cp dist/index.html dist/404.html && cp dist/sw.js dist/service-worker.js",
    "build-splash-css": "postcss assets/splash.css --no-map -u cssnano postcss-cssnext",
    "start": "npm run server:dev",
    "server": "npm run server:dev",
    "server:dev": "cross-env NODE_ENV=development npm run webpack-dev-server -- --inline --profile --watch",
    "server:prod": "serve -s -p 8080 dist",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server --mode development",
    "type-check": "tsc --noEmit",
    "ci": "npm run lint && npm test && npm run type-check"
  },
  "dependencies": {
    "@material-ui/core": "4.5.1",
    "@material-ui/icons": "4.5.1",
    "array-move": "2.2.0",
    "date-fns": "2.6.0",
    "history": "4.10.1",
    "i18next": "18.0.1",
    "i18next-browser-languagedetector": "4.0.0",
    "i18next-chained-backend": "2.0.0",
    "i18next-localstorage-backend": "3.0.0",
    "i18next-xhr-backend": "3.2.0",
    "lodash-es": "4.17.15",
    "prop-types": "15.7.2",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-i18next": "11.0.0",
    "react-loadable": "5.5.0",
    "react-redux": "7.1.1",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "react-sortable-hoc": "1.10.1",
    "redux": "4.0.4",
    "redux-persist": "6.0.0",
    "reselect": "4.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.6.3",
    "@babel/preset-react": "7.6.3",
    "@babel/preset-stage-1": "7.0.0",
    "@types/jest": "24.0.20",
    "@types/lodash-es": "4.17.3",
    "@types/node": "12.11.7",
    "@types/react": "16.9.11",
    "@types/react-collapse": "4.0.2",
    "@types/react-dom": "16.9.3",
    "@types/react-loadable": "5.5.2",
    "@types/react-redux": "7.1.5",
    "@types/react-router-dom": "5.1.0",
    "@types/react-sortable-hoc": "0.6.6",
    "@types/react-transition-group": "4.2.3",
    "@typescript-eslint/eslint-plugin": "2.5.0",
    "@typescript-eslint/parser": "2.5.0",
    "awesome-typescript-loader": "5.2.1",
    "babel-minify-webpack-plugin": "0.3.1",
    "copy-webpack-plugin": "5.0.4",
    "cross-env": "6.0.3",
    "css-loader": "3.2.0",
    "cssnano": "4.1.10",
    "del-cli": "3.0.0",
    "eslint": "6.6.0",
    "eslint-config-prettier": "6.5.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "husky": "3.0.9",
    "jest": "24.9.0",
    "lolex": "5.1.1",
    "mini-css-extract-plugin": "0.8.0",
    "postcss": "7.0.21",
    "postcss-advanced-variables": "3.0.0",
    "postcss-cli": "6.1.3",
    "postcss-extend": "1.0.5",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-nested": "4.1.2",
    "postcss-preset-env": "6.7.0",
    "postcss-reporter": "6.0.1",
    "postcss-scss": "2.0.0",
    "postcss-strip-inline-comments": "0.1.5",
    "prettier": "1.18.2",
    "pretty-quick": "2.0.0",
    "serve": "11.2.0",
    "style-loader": "1.0.0",
    "stylelint": "11.1.1",
    "stylelint-config-recommended": "3.0.0",
    "ts-jest": "24.1.0",
    "typescript": "3.6.4",
    "webpack": "4.41.2",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "3.3.9",
    "webpack-dev-server": "3.9.0",
    "webpack-md5-hash": "0.0.6",
    "webpack-merge": "4.2.2",
    "workbox-webpack-plugin": "4.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
