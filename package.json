{
  "name": "bridge-calc",
  "version": "0.1.0",
  "description": "Webapp for calculating mark in German Bridge",
  "author": "Holi0317 <holliswuhollis@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/Holi0317/bridge-calc.git"
  },
  "bugs": {
    "url": "https://github.com/Holi0317/bridge-calc/issues"
  },
  "homepage": "https://github.com/Holi0317/bridge-calc#readme",
  "main": "dist/main.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test ./node_modules/karma/bin/karma start test/karma.conf.js",
    "webdriver:update": "cross-env ./node_modules/.bin/webdriver-manager update",
    "webdriver:start": "cross-env ./node_modules/.bin/webdriver-manager start",
    "pree2e": "npm run webdriver:update -- --standalone",
    "e2e": "concurrently --success first --kill-others \"npm run e2e:start-when-ready\" \"cross-env WEBPACK_PORT=19876 npm start\"",
    "e2e:start-when-ready": "wait-on --timeout 120000 http-get://localhost:19876/index.html && npm run e2e:start",
    "e2e:start": "cross-env ./node_modules/.bin/protractor test/protractor.conf.js",
    "e2e:live": "npm run e2e:start -- --elementExplorer",
    "lint": "npm run lint:js",
    "lint:js": "eslint --cache webpack-cfg test webpack.config.js",
    "clean": "del-cli dist",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "cross-env NODE_ENV=development npm run webpack -- --progress --profile",
    "prebuild:prod": "npm run clean",
    "build:prod": "cross-env NODE_ENV=production npm run webpack -- --progress --profile",
    "build-splash-css": "node-sass styles/splash.scss | cssnano",
    "start": "npm run server:dev",
    "server": "npm run server:dev",
    "server:dev": "cross-env NODE_ENV=development npm run webpack-dev-server -- --inline --progress --profile --watch",
    "server:prod": "serve -s -p 8080 dist",
    "webpack": "cross-env ./node_modules/.bin/webpack",
    "webpack-dev-server": "cross-env ./node_modules/.bin/webpack-dev-server"
  },
  "aurelia": {
    "build": {
      "resources": []
    },
    "includeDependencies": "aurelia-*"
  },
  "dependencies": {
    "aurelia-bootstrapper-webpack": "1.1.0",
    "aurelia-dependency-injection": "1.3.0",
    "aurelia-event-aggregator": "1.0.1",
    "aurelia-fetch-client": "1.1.1",
    "aurelia-framework": "1.1.0",
    "aurelia-history-browser": "1.0.0",
    "aurelia-loader-webpack": "2.1.0",
    "aurelia-logging": "1.3.0",
    "aurelia-logging-console": "1.0.0",
    "aurelia-mdl-plugin": "1.0.0",
    "aurelia-pal-browser": "1.1.0",
    "aurelia-route-recognizer": "1.1.0",
    "aurelia-router": "1.2.1",
    "aurelia-templating-binding": "1.3.0",
    "aurelia-templating-resources": "1.3.1",
    "aurelia-templating-router": "1.1.0",
    "autobind-decorator": "1.3.4",
    "dexie": "2.0.0-beta.10",
    "es6-symbol": "3.1.0",
    "get-own-property-symbols": "0.9.2",
    "material-design-lite": "1.3.0",
    "reflect-metadata": "0.1.10",
    "sortablejs": "1.5.1",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@easy-webpack/core": "2.0.1",
    "@types/node": "7.0.8",
    "aurelia-protractor-plugin": "1.0.1",
    "aurelia-tools": "1.0.0",
    "aurelia-webpack-plugin": "1.2.2",
    "awesome-typescript-loader": "3.1.2",
    "babel-preset-babili": "0.0.12",
    "babili-webpack-plugin": "0.0.11",
    "concurrently": "3.4.0",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "3.2.3",
    "css-loader": "0.27.1",
    "cssnano-cli": "1.0.5",
    "del-cli": "0.2.1",
    "eslint": "^3.17.1",
    "extract-text-webpack-plugin": "2.1.0",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "karma": "1.5.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-tap": "3.1.1",
    "karma-webpack": "2.0.2",
    "node-sass": "4.5.0",
    "protractor": "5.1.1",
    "sass-loader": "6.0.3",
    "serve": "5.0.3",
    "style-loader": "0.13.2",
    "tape": "4.6.3",
    "tslint": "^4.5.1",
    "typescript": "2.2.1",
    "wait-on": "2.0.2",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.1",
    "webpack-md5-hash": "0.0.5"
  }
}
