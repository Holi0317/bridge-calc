{
  "name": "bridge-calc",
  "version": "2.2.2",
  "description": "Webapp for calculating mark in German Bridge",
  "author": "Holi0317 <holliswuhollis@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://gitlab.com/holi0317/bridge-calc.git"
  },
  "bugs": {
    "url": "https://gitlab.com/holi0317/bridge-calc/issues"
  },
  "homepage": "https://gitlab.com/holi0317/bridge-calc",
  "main": "dist/main.js",
  "scripts": {
    "test": "jest",
    "lint": "npm run lint:js && npm run lint:ts && npm run lint:css",
    "lint:css": "stylelint src/**/*.css assets/splash.css",
    "lint:js": "eslint --ext .jsx,.js --cache webpack-cfg *.config.js src",
    "lint:js:fix": "eslint --ext .jsx,.js --fix --cache webpack-cfg *.config.js src",
    "lint:ts": "tslint -c tslint.json -p tsconfig.json -t stylish",
    "clean": "del-cli dist .tmp",
    "build": "npm run build:prod",
    "prebuild:prod": "npm run clean",
    "build:prod": "cross-env NODE_ENV=production npm run webpack -- --profile --mode production",
    "postbuild:prod": "cp dist/index.html dist/404.html && cp dist/sw.js dist/service-worker.js",
    "build-splash-css": "postcss assets/splash.css --no-map -u cssnano postcss-cssnext",
    "start": "npm run server:dev",
    "server": "npm run server:dev",
    "server:dev": "cross-env NODE_ENV=development npm run webpack-dev-server -- --inline --profile --watch",
    "server:prod": "serve -s -p 8080 dist",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server --mode development",
    "type-check": "tsc --noEmit",
    "ci": "npm run lint && npm test && npm run type-check"
  },
  "dependencies": {
    "@material-ui/core": "1.4.3",
    "@material-ui/icons": "2.0.1",
    "date-fns": "1.29.0",
    "history": "4.7.2",
    "i18next": "11.6.0",
    "i18next-browser-languagedetector": "2.2.3",
    "i18next-localstorage-cache": "1.1.1",
    "i18next-xhr-backend": "1.5.1",
    "lodash-es": "4.17.10",
    "prop-types": "15.6.2",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-i18next": "7.10.1",
    "react-loadable": "5.5.0",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-sortable-hoc": "0.8.3",
    "redux": "4.0.0",
    "redux-persist": "5.10.0",
    "reselect": "3.0.1"
  },
  "devDependencies": {
    "@types/i18next": "8.4.4",
    "@types/i18next-browser-languagedetector": "2.0.1",
    "@types/i18next-xhr-backend": "1.4.1",
    "@types/jest": "23.3.1",
    "@types/lodash-es": "4.17.1",
    "@types/node": "10.5.8",
    "@types/react": "16.4.9",
    "@types/react-dom": "16.0.7",
    "@types/react-i18next": "7.8.2",
    "@types/react-loadable": "5.4.1",
    "@types/react-redux": "6.0.6",
    "@types/react-router-dom": "4.3.0",
    "@types/react-sortable-hoc": "0.6.4",
    "@types/react-transition-group": "2.0.13",
    "awesome-typescript-loader": "5.2.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-minify-webpack-plugin": "0.3.1",
    "babel-plugin-transform-react-remove-prop-types": "0.4.14",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "copy-webpack-plugin": "4.5.2",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "cssnano": "4.0.5",
    "del-cli": "1.1.0",
    "eslint": "5.3.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.5.0",
    "lolex": "2.7.1",
    "mini-css-extract-plugin": "0.4.1",
    "postcss": "7.0.2",
    "postcss-advanced-variables": "2.3.3",
    "postcss-cli": "6.0.0",
    "postcss-extend": "1.0.5",
    "postcss-import": "12.0.0",
    "postcss-loader": "3.0.0",
    "postcss-nested": "3.0.0",
    "postcss-preset-env": "5.3.0",
    "postcss-reporter": "5.0.0",
    "postcss-scss": "2.0.0",
    "postcss-strip-inline-comments": "0.1.5",
    "preload-webpack-plugin": "3.0.0-beta.1",
    "serve": "10.1.2",
    "style-loader": "0.22.1",
    "stylelint": "9.4.0",
    "stylelint-config-standard": "18.2.0",
    "ts-jest": "23.1.3",
    "tslint": "5.11.0",
    "tslint-language-service": "0.9.9",
    "typescript": "3.0.1",
    "webpack": "4.16.5",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.5",
    "webpack-md5-hash": "0.0.6",
    "webpack-merge": "4.1.4",
    "workbox-webpack-plugin": "3.4.1"
  }
}
