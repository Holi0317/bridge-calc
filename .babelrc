{
  "presets": [
    "es2016",
    "es2017",
    "react",
    "stage-1"
  ],
  "plugins": [
    "transform-decorators-legacy",
    ["transform-react-jsx", {
      "pragma": "h"
    }]
  ],

  "env": {
    "production": {
      "plugins": ["transform-react-remove-prop-types"]
    },
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  }
}
